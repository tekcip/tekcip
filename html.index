        }
        .chip3 .chip3-0{
            background: var(--main-color);
            border-radius: 100px;
            min-width: 500px;
            min-height: 1570px;
        }
        .chip3 .chip3-1{
            background: var(--other-color);
            border-radius: 100px;
            min-width: 500px;
            min-height: 1570px;
        }
        .chip4{
            display: grid;
            grid-template-columns: repeat(2, 1fr);
